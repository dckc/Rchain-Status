{
  "extends": "airbnb-base",
  "rules": {
    // no-use-before-define is pretty painful.
    // I suppose it's consistent with E and Monte,
    // but python habits die hard, and I like
    // to write code top-down.
    // https://github.com/airbnb/javascript/issues/273
    "no-use-before-define": [2, "nofunc"],
    "no-console": 0
  }
}
